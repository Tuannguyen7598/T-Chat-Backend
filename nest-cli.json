{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "apps/user-server/src",
	"monorepo": true,
	"root": "apps/user-server",
	"compilerOptions": {
		"webpack": true,
		"tsConfigPath": "apps/user-server/tsconfig.app.json"
	},
	"projects": {
		"api-gate-way": {
			"type": "application",
			"root": "apps/api-gate-way",
			"entryFile": "main",
			"sourceRoot": "apps/api-gate-way/src",
			"compilerOptions": {
				"tsConfigPath": "apps/api-gate-way/tsconfig.app.json"
			}
		},
		"socket-server": {
			"type": "application",
			"root": "apps/socket-server",
			"entryFile": "main",
			"sourceRoot": "apps/socket-server/src",
			"compilerOptions": {
				"tsConfigPath": "apps/socket-server/tsconfig.app.json"
			}
		},
		"message-server": {
			"type": "application",
			"root": "apps/message-server",
			"entryFile": "main",
			"sourceRoot": "apps/message-server/src",
			"compilerOptions": {
				"tsConfigPath": "apps/message-server/tsconfig.app.json"
			}
		},
		"conversation-server": {
			"type": "application",
			"root": "apps/conversation-server",
			"entryFile": "main",
			"sourceRoot": "apps/conversation-server/src",
			"compilerOptions": {
				"tsConfigPath": "apps/conversation-server/tsconfig.app.json"
			}
		},
		"notifi-server": {
			"type": "application",
			"root": "apps/notifi-server",
			"entryFile": "main",
			"sourceRoot": "apps/notifi-server/src",
			"compilerOptions": {
				"tsConfigPath": "apps/notifi-server/tsconfig.app.json"
			}
		}
	}
}
